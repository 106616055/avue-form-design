    "v-jsoneditor": "^1.2.1",
    "vuedraggable": "^2.23.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.5",
    "@vue/cli-plugin-eslint": "^4.4.5",
    "@vue/cli-service": "^4.4.5",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/sscfaith/avue-form-design"
